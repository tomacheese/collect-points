    "axios": "1.6.8",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "17.2.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "52.0.0",
    "prettier": "3.2.5",
