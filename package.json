    "axios": "1.7.7",
    "eslint": "9.10.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-n": "17.10.2",
    "eslint-plugin-promise": "7.1.0",
    "prettier": "3.3.3",
    "puppeteer-core": "23.3.0",
    "run-z": "2.0.0",
    "rxjs": "7.8.1",
    "tsx": "4.19.1",
