    "axios": "1.7.7",
    "eslint": "9.13.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.12.0",
    "eslint-plugin-promise": "7.1.0",
    "prettier": "3.3.3",
    "puppeteer-core": "23.6.1",
    "run-z": "2.1.0",
    "rxjs": "7.8.1",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
